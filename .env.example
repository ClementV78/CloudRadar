PROFILE=CloudRadarTerraformUser
ACCOUNT_ID=123456789012
MFA_DEVICE_NAME=your_mfa_device_name
AWS_TERRAFORM_ROLE_ARN=arn:aws:iam::123456789012:role/CloudRadarTerraformRole
# Optional override for bootstrap runs:
# ROLE_ARN=arn:aws:iam::123456789012:role/CloudRadarBootstrapRole
SESSION_NAME=cloudradar-terraform
DURATION_SECONDS=3600
# Monitoring: Grafana and Prometheus passwords
# Leave empty to have Terraform generate random passwords
GRAFANA_ADMIN_PASSWORD=
PROMETHEUS_AUTH_PASSWORD=

# Terraform variable exports (for local terraform apply)
export TF_VAR_grafana_admin_password="${GRAFANA_ADMIN_PASSWORD}"
export TF_VAR_prometheus_auth_password="${PROMETHEUS_AUTH_PASSWORD}"